{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "A template Discord Bot for Projects of Hulkermon#1337.",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "nodemon src/index.ts",
    "build": "tsc -p .",
    "registerCommands": "tsc registerCommands.ts && node registerCommands.js && rm registerCommands.js"
  },
  "nodemonConfig": {
    "ignore": [
      "environments/",
      "db/"
    ],
    "exec": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hulkerman/discord-bot-template.git"
  },
  "keywords": [
    "Discord",
    "Bot",
    "Template"
  ],
  "author": "Hulkermon#1337",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hulkerman/discord-bot-template/issues"
  },
  "homepage": "https://github.com/Hulkerman/discord-bot-template#readme",
  "dependencies": {
    "@discordjs/opus": "^0.3.3",
    "axios": "^0.21.0",
    "bufferutil": "^4.0.2",
    "discord.js": "^12.5.1",
    "erlpack": "^0.1.3",
    "libsodium-wrappers": "^0.7.8",
    "sqlite3": "^5.0.0",
    "utf-8-validate": "^5.0.3",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/request": "^2.48.5",
    "@types/sqlite3": "^3.1.6",
    "jasmine-core": "^3.6.0",
    "node-gyp": "^7.1.2",
    "nodemon": "^2.0.6",
    "open": "^7.3.0",
    "prompt": "^1.0.0",
    "ts-node": "^9.0.0",
    "typescript": "4.0.5"
  }
}
